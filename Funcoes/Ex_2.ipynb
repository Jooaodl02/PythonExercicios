    "#EXERCICIO 6\n",
    "\n",
    "h = float(input(\"Informe as horas: \"))\n",
    "m = float(input(\"Informe os minutos: \"))\n",
    "s = float(input(\"Informe os segundos: \"))\n",
    "\n",
    "def horario(h, m, s):\n",
    "    s1 = h*3600\n",
    "    s2 = m*60\n",
    "    return print(f\" O horario total em segundos é, {s + s1 + s2}\" )\n",
    "\n",
    "horario(h, m, s)"
